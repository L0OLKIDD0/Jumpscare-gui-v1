local function INOUST_fake_script() -- Scare_2.LocalScript 
	local script = Instance.new('LocalScript', Scare_2)
 
	script.Parent.MouseButton1Click:Connect(function()
		while wait(0.1) do
			local h = Instance.new("Hint")
			h.Parent = game.Workspace
			h.Text = ("!(/24bujnwadsSADHBIN")
			wait(0.1)
			h.Text = ("VABJSNDHGBJN&as455DSA")
			wait(9.4)
			h:Destroy()
		end
	end)
end
